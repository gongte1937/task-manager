{
  "name": "next-client",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "next-client",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/next-client",
        "root": "next-client",
        "fileReplacements": []
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "next-client:build",
        "dev": true
      }
    },
    "dev": {
      "executor": "@nx/next:dev-server",
      "options": {
        "buildTarget": "next-client:build"
      }
    }
  }
}
